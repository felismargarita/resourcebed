<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.felis.resource.PictureMapper">
    <resultMap id="PictureMapper" type="com.felis.resource.Picture">
        <id property="id" column="id"/>
        <result property="base64" column="base64"/>
        <result property="md5" column="md5"/>
        <result property="createdTime" column="created_time"/>
    </resultMap>

    <insert id="insert" parameterType="com.felis.resource.Picture">
        insert into re_picture (base64,md5,created_time) values(#{base64},#{md5},now())
    </insert>

    <select id="selectByHash" parameterType="string" resultMap="PictureMapper">
        select * from re_picture where md5=#{md5}
    </select>




</mapper>